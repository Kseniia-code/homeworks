						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"My user name equal to expected\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.document.name).to.eql(pm.collectionVariables.get(\"myUserName\"));\r",
									"});\r",
									"\r",
									"pm.test(\"My user age equal to expected\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.document.age).to.eql(+pm.collectionVariables.get(\"myUserAge\"));\r",
									"});\r",
									"pm.test(\"My user email equal to expected\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.document.email).to.eql(pm.collectionVariables.get(\"myUserEmail\"));\r",
									"});\r",
									"\r",
